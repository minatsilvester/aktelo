<%= form_with(model: [@organization, @organization.boards.build], local: true) do |form| %>

<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name, class: "form-control" %>
</div>

<div class="form-group">
  <%= form.label :about %>
  <%= form.text_area :about, class: "form-control" %>
</div>

<div class="form-group">
  <%= form.submit "Create Board", class: "btn btn-primary" %>
</div>

<% end %>
